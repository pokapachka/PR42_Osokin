<Window x:Class="ПР42_Осокин.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ПР42_Осокин"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontFamily="Century Gothic">
    <Grid>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden" Margin="5,55,5,5" Background="#FFFAFAFA"/>
        <Grid VerticalAlignment="Top" Height="50" Background="White">
            <Grid.Effect>
                <DropShadowEffect Direction="270" BlurRadius="5" ShadowDepth="2" Opacity="0.5"/>
            </Grid.Effect>
            <Label Content="Вещи для дома" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="15pt"/>
            <Image Width="30" Height="30" Margin="10" VerticalAlignment="Top" HorizontalAlignment="Left" Source="/Images/ic_shop.png" MouseDown="OpenIndex"/>
        </Grid>
    </Grid>
</Window>
